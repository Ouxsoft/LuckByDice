{
  "family": "luckbydice-fargate",
  "networkMode": "awsvpc",
  "executionRoleArn": "arn:aws:iam::124653296725:role/ecsTaskExecutionRole",
  "containerDefinitions": [
    {
      "name": "luckbydice",
      "image": "124653296725.dkr.ecr.us-east-2.amazonaws.com/luckbydice",
      "portMappings": [
        {
          "containerPort": 80,
          "hostPort": 80,
          "protocol": "tcp"
        }
      ],
      "essential": true,
      "entryPoint": [
        "sh",
        "-c"
      ],
      "command": [
        "cd public && php -S 0.0.0.0:80"
      ]
    }
  ],
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "cpu": "256",
  "memory": "512"
}